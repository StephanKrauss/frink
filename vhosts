<VirtualHost *:80>
    DocumentRoot "c:/xampp/htdocs/frink/public"
    ServerName www.frink.de
	DirectoryIndex index.html index.php
    ErrorLog "c:/xampp/apache/logs/frink_err.log"
    CustomLog "c:/xampp/apache/logs/frink.log" combined
    <Directory "c:/xampp/htdocs/frink/public">
        Options Indexes FollowSymLinks Includes ExecCGI
    	Order allow,deny
    	Allow from all
    	AllowOverride All
    </Directory>
</VirtualHost>